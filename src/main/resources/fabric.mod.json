    "issues": "https://github.com/Enchanted-Games/falling-block-particles/issues"
  },
  "license": "CC0-1.0",
  "icon": "assets/falling-block-particles/icon.png",
  "environment": "client",
  "entrypoints": {
    "client": [
      "games.enchanted.fallingBlockParticles.FallingBlockParticlesClient"
    ]
  },
  "mixins": ["falling-block-particles.mixins.json"],
  "depends": {
    "fabricloader": ">=0.14.21",
